body {
  margin: 0;
  padding: 0; }

.contact {
  width: 320px;
  margin: 0 4px; }
  .contact #msgList {
    -moz-border-radius: 8px;
    -webkit-border-radius: 8px;
    -khtml-border-radius: 8px;
    border-radius: 8px;
    margin: 4px 0;
    float: left;
    width: 320px;
    max-height: 320px;
    overflow: auto;
    border: 1px solid #bfbfbf;
    -webkit-box-shadow: 0 0 4px #bfbfbf; }
    .contact #msgList ul {
      margin: 4px 0;
      list-style-type: none;
      padding: 0 4px; }
      .contact #msgList ul .info {
        margin: 2px;
        padding: 2px;
        -moz-border-radius: 4px;
        -webkit-border-radius: 4px;
        -khtml-border-radius: 4px;
        border-radius: 4px;
        border: 1px solid #bfbfbf;
        -webkit-box-shadow: 0 0 4px #bfbfbf;
        font-size: 70%; }
      .contact #msgList ul input#name {
        width: 304px;
        font-size: 16px;
        -moz-border-radius: 4px;
        -webkit-border-radius: 4px;
        -khtml-border-radius: 4px;
        border-radius: 4px;
        border: 1px solid #bfbfbf;
        -webkit-box-shadow: 0 0 4px #bfbfbf; }
      .contact #msgList ul li.owner, .contact #msgList ul li.system {
        font-size: 16px;
        margin: 4px 0;
        padding: 4px;
        width: 304px;
        background-color: #d9f2fe;
        -moz-border-radius: 8px;
        -webkit-border-radius: 8px;
        -khtml-border-radius: 8px;
        border-radius: 8px;
        border: 1px solid #bfbfbf;
        -webkit-box-shadow: 0 0 4px #bfbfbf; }
      .contact #msgList ul li.owner {
        background-color: #fff9da; }
  .contact #msg {
    width: 316px; }
    .contact #msg textarea#msg {
      font-size: 16px;
      -moz-border-radius: 8px;
      -webkit-border-radius: 8px;
      -khtml-border-radius: 8px;
      border-radius: 8px;
      border: 1px solid #bfbfbf;
      -webkit-box-shadow: 0 0 4px #bfbfbf;
      width: 100%;
      line-height: 20px;
      height: 40px; }
